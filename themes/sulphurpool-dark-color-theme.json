    "tokenColors": [
        {
            "scope": [
                "string",
                "string.quoted",
                "string.template"
            ],
            "settings": {
                "foreground": "#ac9738"
            }
        },
        {
            "scope": [
                "keyword",
                "storage.type"
            ],
            "settings": {
                "foreground": "#6679cc"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#3d8fd1"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#dfe2f1"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#c76a28"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#6a7293"
            }
        },
        {
            "scope": [
                "punctuation.bracket",
                "punctuation.delimiter"
            ],
            "settings": {
                "foreground": "#979db4"
            }
        },
        {
            "scope": "entity.name.type",
            "settings": {
                "foreground": "#c08b2f"
            }
        },
        {
            "scope": "constant",
            "settings": {
                "foreground": "#ac9739"
            }
        },
        {
            "scope": "support.type",
            "settings": {
                "foreground": "#c08b2f"
            }
        },
        {
            "scope": [
                "punctuation.special",
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#9b6279"
            }
        },
        {
            "scope": "variable.special",
            "settings": {
                "foreground": "#6679cc"
            }
        },
        {
            "scope": [
                "variable.parameter",
                "variable.other.object.property"
            ],
            "settings": {
                "foreground": "#dfe2f1"
            }
        },
        {
            "scope": [
                "variable.other.constant",
                "variable.other.enummember"
            ],
            "settings": {
                "foreground": "#ac9739"
            }
        },
        {
            "scope": [
                "entity.name.class",
                "entity.name.type.class"
            ],
            "settings": {
                "foreground": "#c08b2f"
            }
        },
        {
            "scope": [
                "entity.name.tag",
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#3d8fd1"
            }
        },
        {
            "scope": [
                "meta.import",
                "meta.export"
            ],
            "settings": {
                "foreground": "#6679cc"
            }
        },
        {
            "scope": [
                "storage.modifier",
                "storage.type.function"
            ],
            "settings": {
                "foreground": "#6679cc"
            }
        },
        {
            "scope": [
                "string.regexp",
                "string.escape"
            ],
            "settings": {
                "foreground": "#ac9738"
            }
        }
    ]
}